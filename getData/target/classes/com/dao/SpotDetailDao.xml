        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.SpotDetailDao">


    <insert id="insertSpotDetailData" parameterType="com.entity.SpotDetail">
        insert into t_spot_detail(title,info,picture) values(#{spotDetail.title},#{spotDetail.info},#{spotDetail.picture});
    </insert>

</mapper>